<% include partials/head.ejs %>

<header class="">
  <h1>Teams</h1>
</header>

<section>
  <a href="/game-update">update game page</a>
  <a href="/games">GAMES</a>
  <ul class="teamlist">
    <% teams.forEach(function (obj) { %>
    <li class="team">
      <a href="/teams/<%= obj.team.id %>">
        <h3 class="teamName"> <%= obj.team.short_name %> </h3>
      </a>
    </li>
    <% }) %>
  </ul>
</section>

