<% include partials/head.ejs %>

<header class="">
  <div class="background"></div>
  <div class="header-section">
      <!--<button class="nav-btn"><span>menu</span></button>-->
    <button class=""><a href="/games"><</a></button>
    <h1>Swiss sSwiss</h1>
  </div>
</header>

<section class="swiss-standings">
  <p>Last updated: [DATE HERE]</p>
  <h2>ROUND NUMBER <%= round_number %></h2>
  <table>
    <tr>
      <th>Ranking</th>
      <th>TEAM</th>
      <th>Swiss Points</th>
    </tr>
    <% data.forEach(function (obj) { %>
    <tr>
      <td><%= obj.ranking %>.</td>
      <td><a href="/teams/<%= obj.team.id %>"><%= obj.team.short_name %></a></td>
      <td><%= obj.swiss_score %></td>
    </tr>
    <% }) %>
  </table>

</section>

<% include partials/footer.ejs %>
